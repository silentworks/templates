{
	"name": "sveltekit-supabase-daisyui-zod",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "concurrently \"npm run dev:css\" \"vite dev\"",
		"dev:macos": "concurrently \"npm run dev:css\" \"vite dev --mode macos\"",
		"dev:windows": "concurrently \"npm run dev:css\" \"vite dev --mode windows\"",
		"dev:css": "tailwindcss -w -i ./src/tailwind.css -o static/app.css",
		"build": "npm run build:css && vite build",
		"build:css": "tailwindcss -m -i ./src/tailwind.css -o static/app.css",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.13",
		"concurrently": "^7.6.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.20",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.2",
		"svelte-preprocess": "^5.0.0",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.1"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.8.6",
		"@supabase/supabase-js": "^2.2.0",
		"daisyui": "^2.43.2",
		"zod": "^3.20.2"
	}
}
